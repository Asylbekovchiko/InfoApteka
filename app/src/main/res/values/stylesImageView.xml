<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ImageViewStyle">
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
        <item name="android:clickable">true</item>
        <item name="colorControlHighlight">@color/light</item>
    </style>

    <style name="ShapeAppearance.CornerTop_8" parent="">
        <item name="cornerSizeTopLeft">@dimen/dp_1x</item>
        <item name="cornerSizeTopRight">@dimen/dp_1x</item>
    </style>

    <style name="ShapeAppearance.Corner_12" parent="">
        <item name="cornerSize">12dp</item>
    </style>

    <style name="ShapeAppearance.Corner_8" parent="">
        <item name="cornerSize">8dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Corner_bottom_12" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomRight">12dp</item>
        <item name="cornerSizeBottomLeft">12dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Corner_bottom_16" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomRight">16dp</item>
        <item name="cornerSizeBottomLeft">16dp</item>
    </style>

    <style name="ShapeAppearanceOverlay" parent="">
        <item name="android:layout_height">160dp</item>
        <item name="android:layout_width">160dp</item>
        <item name="strokeColor">@color/light_blue_D4E3FF</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.Size_92" parent="">
        <item name="android:layout_height">@dimen/logo_seller_size</item>
        <item name="android:layout_width">@dimen/logo_seller_size</item>
        <item name="strokeColor">@color/light_blue_D4E3FF</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

</resources>